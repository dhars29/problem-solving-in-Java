import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        int p =sc.nextInt();
        int Quotient =t/p;
        int remainder =t%p;
        System.out.println("Total teams formed: "+Quotient);
        
        System.out.println("Players left out: "+ remainder);
        sc.close();
    
    }
}
